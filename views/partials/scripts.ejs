    <script src='/prism.js'></script>
    <script>
        const h3s = document.querySelectorAll('h3');
        for(let h3 of h3s)
        {
            h3.addEventListener('click', ()=>{
                h3.nextSibling.nextSibling.classList.toggle('hide')
            })
        }
    </script>